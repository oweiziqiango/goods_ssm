<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cqupt.goods_ssm.dao.TAdminDAOExtend" >
  <select id="findAmdinByAdminnameAndpwd" parameterType="com.cqupt.goods_ssm.domain.extend.TAdminExtend" 
  resultType="com.cqupt.goods_ssm.domain.extend.TAdminExtend">
  select * from t_admin
  where adminname = #{adminname} 
  and adminpwd = #{adminpwd}
  </select>
</mapper>